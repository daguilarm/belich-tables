{
    "name": "daguilarm/belich-tables",
    "description": "A dynamic table component for Laravel Livewire",
    "keywords": [
        "livewire",
        "alpineJS",
        "package",
        "datatables",
        "tables",
        "laravel",
        "daguilarm"
    ],
    "homepage": "https://github.com/daguilarm/belich-tables",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dami√°n Aguilar",
            "email": "damian.aguilarm@gmail.com",
            "homepage": "https://daguilar.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "daguilarm/belich-api": "^1.0",
        "livewire/livewire": "^2.3",
        "maatwebsite/excel": "^3.1.3",
        "mattlibera/livewire-flash": "^0.5.0",
        "symfony/http-kernel": "^5.0"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "nunomaduro/phpinsights": "dev-master",
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Daguilarm\\BelichTables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daguilarm\\BelichTables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daguilarm\\BelichTables\\BelichTablesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
